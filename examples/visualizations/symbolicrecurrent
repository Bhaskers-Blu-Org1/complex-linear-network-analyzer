digraph structs {
	graph [constraint=true nodesep=2 overlap=false rankdir=TB ranksep=0.5 splines=true]
	node [shape=record]
	a [label=a]
	b [label=b]
	c [label=c]
	d [label=d]
	a -> b [label="a0.95, p0.0 + ph1*1, d1.0"]
	b -> c [label="a1.0 * amp1**1, p0.5, d1.0"]
	c -> d [label="a0.95, p0.5, d0.0 + del1*1"]
	d -> a [label="a0.95, p0.5, d1.0"]
}
